* {
        border: none;
        border-radius: 0px;
        font-family: "IosevkaTerm Nerd Font" ;
        font-size: 10px;
        font-weight: bold;
        min-height: 10px;
        margin-top: 2px;
        margin-bottom: 2px;
        margin-left: 2px;
        margin-right: 2px;
    }

 window#waybar {
    opacity: 1;
    background-color: #19161e;
    font-weight: bold;
}

#workspaces {
  background: transparent;
  border-radius: 5px;
}

#workspaces button:hover {
  background: #5E69AF;
}

    button {
        all: unset;
        background-color: #8698c0;
        color: #282828;
        border-radius: 0px;
        border: none;
        border-bottom: 8px solid #8698c0;
        border-top: 8px solid #8698c0;
    }

    #clock {
        background-color: #8698c0;
        color: #282828;
        font-weight: bold;
        border: none;
        border-bottom: 8px solid #8698c0;
        border-top: 8px solid #8698c0;
    }

    #window {
        background-color: #8698c0;
        color: #282828;
        font-size: 2px;
        font-weight: bold;
        border: none;
        border-bottom: 8px solid #8698c0;
        border-right: 8px solid #8698c0;
        border-top: 8px solid #8698c0;
    }
    
